{
	"info": {
		"_postman_id": "c02-broker-assign-test",
		"name": "InterDev - C02 Broker Assign",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Assign Broker to Request",
			"request": {
				"method": "PATCH",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"brokerId\": \"{{broker_id}}\"\n}"
				},
				"url": {
					"raw": "{{base_url}}/project-requests/{{request_id}}/assign",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"project-requests",
						"{{request_id}}",
						"assign"
					]
				}
			},
			"response": []
		}
	],
	"variable": [
		{
			"key": "base_url",
			"value": "http://localhost:3000",
			"type": "string"
		},
		{
			"key": "request_id",
			"value": "Replace with Request ID from DB",
			"type": "string"
		},
		{
			"key": "broker_id",
			"value": "Replace with Broker ID from DB",
			"type": "string"
		}
	]
}
