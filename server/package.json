    "generate:keys": "node scripts/generate-keys.js",
    "generate:ssl": "ts-node src/scripts/generate-ssl.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm migration:create",
    "migration:generate": "yarn build && yarn typeorm migration:generate -d dist/data-source.js",
    "migration:run": "yarn build && yarn typeorm migration:run -d dist/data-source.js",
    "migration:revert": "yarn typeorm migration:revert -d dist/data-source.js",
    "migration:show": "typeorm migration:show -d dist/data-source.js"
